msgid "Priority:"
msgstr "Προτεραιότητα:"

#: kdesudo/kdesudo.cpp:209
msgid "realtime:"
msgstr "Σε πραγματικό χρόνο:"

#: kdesudo/kdesudo.cpp:255
msgid "Command:"
msgstr "Εντολή:"

